{
  "name": "@marianmeres/clog",
  "version": "0.1.3",
  "description": "On top of console.log",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "unpkg": "./dist/index.umd.js",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist/*",
    "prettier": "prettier --write \"{src,tests}/**/*.{js,ts,json}\"",
    "release": "npx release",
    "test": "node src/index.test.js",
    "build": "npm run clean && microbundle src/index.js --no-sourcemap --target node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marianmeres/clog.git"
  },
  "keywords": [
    "console.log"
  ],
  "author": "Marian Meres <marian@meres.sk>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marianmeres/clog/issues"
  },
  "homepage": "https://github.com/marianmeres/clog#readme",
  "devDependencies": {
    "@marianmeres/release": "^0.0.7",
    "@types/node": "^18.11.9",
    "microbundle": "^0.15.1",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2"
  }
}
